<meta charset="utf-8">
{% if viewport %}
<meta name="viewport" content="{{ viewport }}">
{% else %}
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
{% endif %}

<title>{{ title }}</title>

{% if meta.author.name %}
<meta name="author" content="{{ meta.author.name }}">
{% endif %}
<meta name="description" content="{{ description }}">
{% if meta.keywords %}
<meta name="keywords" content="{{ meta.keywords }}">
{% endif %}

{% if meta.mobile-theme-col %}
<meta name="theme-color" content="{{ meta.mobile-theme-col }}">
{% endif %}

{% if meta.fb_app_id %}
<meta property="fb:app_id" content="{{ meta.fb_app_id }}">
{% endif %}



{% if common_ext_css %}{% for css in common_ext_css %}
{% if css.sri %}
<link rel="stylesheet" href="{{ css.href }}" integrity="{{ css.sri }}" crossorigin="anonymous">
{% elseif css.href %}
<link rel="stylesheet" href="{{ css.href }}">
{% else %}
<link rel="stylesheet" href="{{ css }}">
{% endif %}
{% endfor %}{% endif %}

{% if common_css %}{% for css in common_css %}
<link rel="stylesheet" href="{{ css | url }}">
{% endfor %}{% endif %}



