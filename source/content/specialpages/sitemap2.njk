<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for post in collections.all %}
    <url>
      <loc>{{ post.url }}</loc>
      <lastmod>{{ post.date | date("YYYY-MM-DD[T]HH:mm:ssZ") }}</lastmod>
      <priority>1.0</priority>
    </url>
  {% endfor %}
</urlset>
